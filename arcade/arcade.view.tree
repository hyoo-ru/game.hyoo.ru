	Realm $hyoo_game_realm
		map <= map
		spawn_pos => spawn_pos
		map_rows => map_rows
		map_width => map_width
		map_height => map_height
	actors /$hyoo_game_actor
		<= Guy $hyoo_game_actor
			Realm <= Realm
			pos_spawn <= spawn_pos
			pos => guy_pos
			turn_left? => turn_left?
			turn_right? => turn_right?
			move_forward? => move_forward?
			move_backward? => move_backward?
			move_left? => move_left?
			move_right? => move_right?
			angle => guy_angle
		<= Healer $hyoo_game_actor_ghost
			Realm <= Realm
			pos_spawn <= spawn_pos
	sub /
		<= View $mol_view sub /
			<= Guy_eye $hyoo_game_eye
				Realm <= Realm
				pos <= guy_pos
				angle <= guy_angle
				wireframe <= wireframe
				objects /$mol_3d_object
					<= Floor $mol_3d_object
						shape <= Square
						texture <= Floor_image $mol_3d_image
							uri \hyoo/game/arcade/texture/ground.jpg
						transform <= floor_trans $mol_3d_mat4
					<= Ceil $mol_3d_object
						shape <= Square
						texture <= Floor_image
						transform <= ceil_trans $mol_3d_mat4
					^ walls
					^ avatars
		<= Hud $mol_page
			title <= title \$hyoo_game_arcade
			tools /
				<= Wireframe $mol_check_box
					title \Wireframe
					checked? <=> wireframe? false
				<= Sources $mol_link_source
					uri \https://github.com/hyoo-ru/game.hyoo.ru
			foot /
				<= stat \
	plugins /
		<= Control $mol_keyboard_state
			key *
				W? <=> move_forward?
				A? <=> move_left?
				S? <=> move_backward?
				D? <=> move_right?
				Q? <=> turn_left?
				E? <=> turn_right?
				up? <=> move_forward?
				left? <=> turn_left?
				down? <=> move_backward?
				right? <=> turn_right?
				pageUp? <=> move_left?
				pageDown? <=> move_right?
	-
	Square $mol_3d_shape_square
	Image* $mol_3d_image
		uri <= image_uri* \
	walls /$mol_3d_object
	avatars /$mol_3d_object
	Wall* $mol_3d_object
		shape <= Square
		texture <= wall_image* $mol_3d_image
		transform <= wall_trans* $mol_3d_mat4
	Avatar* $mol_3d_object
		shape <= Square
		texture <= avaatr_image* $mol_3d_image
			uri \hyoo/game/arcade/texture/healer/0.png
		transform <= avatar_trans* $mol_3d_mat4
